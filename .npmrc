# ZeroPoint Node - npm Configuration

# Package access (public for open source)
access=public

# Registry
registry=https://registry.npmjs.org/

# Git tag version
git-checks=true

# Package verification
audit=false

# Save exact versions
save-exact=true

# Package lock file
package-lock=true

# Ignore scripts for security
ignore-scripts=false

# Log level
loglevel=warn

# Progress bar
progress=true

# Fund message
fund=false

# Audit level
audit-level=moderate 