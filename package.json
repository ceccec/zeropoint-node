{
  "name": "zeropoint-node",
  "version": "1.0.1",
  "description": "ZeroPoint Node - Decentralized Vortex Math & Toroidal Consciousness Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "docs/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint:js": "eslint --env node --env es2021 --parser-options '{\"sourceType\": \"module\"}' --no-eslintrc scripts server.js",
    "lint:ts": "eslint --env node --env es2021 --parser @typescript-eslint/parser --parser-options '{\"sourceType\": \"module\"}' --plugin @typescript-eslint --ext .ts,.tsx src",
    "lint": "npm run lint:js && npm run lint:ts",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:ts -- --fix",
    "start": "node dist/index.js",
    "browser:dev": "webpack serve --mode development",
    "browser:build": "webpack --mode production",
    "browser:prod": "webpack --mode production && npm run start",
    "analyze": "node scripts/performance-monitor.js",
    "analyze:bundle": "ANALYZE=true npm run browser:build",
    "seo:check": "node scripts/performance-monitor.js --seo",
    "performance:check": "node scripts/performance-monitor.js --performance",
    "accessibility:check": "node scripts/performance-monitor.js --accessibility"
  },
  "keywords": [
    "zeropoint",
    "vortex-math",
    "toroidal",
    "consciousness",
    "decentralized",
    "p2p",
    "metaphysics",
    "mathematics"
  ],
  "author": "ZeroPoint Collective",
  "license": "MIT",
  "dependencies": {
    "crypto-js": "4.2.0",
    "stimulus": "3.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/css": "0.10.0",
    "@eslint/js": "9.31.0",
    "@eslint/json": "0.13.0",
    "@eslint/markdown": "7.0.0",
    "@types/crypto-js": "4.2.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.0.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "buffer": "6.0.3",
    "compression-webpack-plugin": "10.0.0",
    "crypto-browserify": "3.12.1",
    "eslint": "8.57.1",
    "eslint-plugin-css": "0.11.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-markdown": "5.1.0",
    "events": "3.3.0",
    "globals": "16.3.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "30.0.4",
    "lslint": "0.0.0",
    "path-browserify": "1.0.1",
    "postcss": "8.5.6",
    "postcss-loader": "7.3.4",
    "prettier": "^3.1.0",
    "querystring-es3": "0.2.1",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.36.0",
    "url": "0.11.4",
    "util": "0.12.5",
    "webpack": "5.99.9",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
